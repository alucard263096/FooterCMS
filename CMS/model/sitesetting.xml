<?xml version="1.0" encoding="utf-8" ?>
<root>
  <name>Website Setting</name>
  <tablename>tb_website_base</tablename>
  <nolist>1</nolist>
  <fields>
    <field>
      <name>Website Name</name>
      <key>website_name</key>
      <type>text</type>
    </field>
    <field>
      <name>Browser Logo</name>
      <key>favfile</key>
      <type>upload</type>
      <uploadmodule>fav</uploadmodule>
    </field>
    <field>
      <name>Contacter</name>
      <key>contacter</key>
      <type>text</type>
    </field>
    <field>
      <name>QQ</name>
      <key>qq</key>
      <type>text</type>
    </field>
    <field>
      <name>Email</name>
      <key>email</key>
      <type>text</type>
    </field>
    <field>
      <name>Mobile</name>
      <key>mobile</key>
      <type>text</type>
    </field>
    <field>
      <name>Phone</name>
      <key>phone</key>
      <type>text</type>
    </field>
    <field>
      <name>Seo Title</name>
      <key>seo_title</key>
      <type>longtext</type>
    </field>
    <field>
      <name>Seo Keywords</name>
      <key>seo_keywords</key>
      <type>longtext</type>
    </field>
    <field>
      <name>Seo description</name>
      <key>seo_description</key>
      <type>longtext</type>
    </field>
  </fields>
</root>